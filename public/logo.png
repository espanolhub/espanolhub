<svg width="512" height="512" viewBox="0 0 512 512" xmlns="http://www.w3.org/2000/svg">
  <!-- Background gradient -->
  <defs>
    <linearGradient id="bgGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#2563EB;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#7C3AED;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#EC4899;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="capGrad" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#FFFFFF;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#F3F4F6;stop-opacity:0.9" />
    </linearGradient>
  </defs>
  
  <!-- Rounded square background -->
  <rect x="0" y="0" width="512" height="512" rx="120" fill="url(#bgGrad)"/>
  
  <!-- Graduation cap icon -->
  <g transform="translate(256, 256)">
    <!-- Cap base -->
    <polygon points="-120,-40 0,-80 120,-40 0,0" fill="url(#capGrad)" stroke="#1E293B" stroke-width="8"/>
    <!-- Cap top -->
    <rect x="-140" y="-50" width="280" height="20" rx="4" fill="url(#capGrad)" stroke="#1E293B" stroke-width="6"/>
    <!-- Tassel -->
    <line x1="120" y1="-40" x2="120" y2="60" stroke="#C8102E" stroke-width="8" stroke-linecap="round"/>
    <circle cx="120" cy="70" r="16" fill="#C8102E"/>
    <!-- Book accent -->
    <rect x="-60" y="20" width="50" height="70" rx="4" fill="#FFFFFF" opacity="0.9" stroke="#1E293B" stroke-width="4"/>
    <rect x="-10" y="20" width="50" height="70" rx="4" fill="#FFFFFF" opacity="0.8" stroke="#1E293B" stroke-width="4"/>
    <rect x="40" y="20" width="50" height="70" rx="4" fill="#FFFFFF" opacity="0.7" stroke="#1E293B" stroke-width="4"/>
  </g>
</svg>